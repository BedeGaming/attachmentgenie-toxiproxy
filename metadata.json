{
  "name": "attachmentgenie-toxiproxy",
  "version": "1.0.1",
  "author": "bram vogelaar <bram@attachmentgenie.com>",
  "summary": "Setup toxiproxy",
  "license": "MIT",
  "source": "https://github.com/attachmentgenie/attachmentgenie-toxiproxy.git",
  "project_page": "https://github.com/attachmentgenie/attachmentgenie-toxiproxy",
  "issues_url": "https://github.com/attachmentgenie/attachmentgenie-toxiproxy/issues",
  "tags": [
    "toxiproxy"
  ],
  "operatingsystem_support": [
    {
